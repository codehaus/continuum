<form>
  <id>user</id>
  <title-key>user.title</title-key>
  <action>login</action>
  <elements>
    <element>
      <id>login.username</id>
      <validator>default</validator>
      <message-key>user.name</message-key>
      <label-key>user.name</label-key>
      <error-message-key>login.username.error</error-message-key>
      <type>text</type>
    </element>
    <element>
      <id>user.lastname</id>
      <validator>default</validator>
      <message-key>login.password.message</message-key>
      <label-key>user.lastname</label-key>
      <error-message-key>login.password.error</error-message-key>
      <type>password</type>
    </element>
    <element>
      <id>login.password</id>
      <validator>default</validator>
      <message-key>login.password.message</message-key>
      <label-key>login.password.label</label-key>
      <error-message-key>login.password.error</error-message-key>
      <type>password</type>
    </element>
    <element>
      <id>login.password</id>
      <validator>default</validator>
      <message-key>login.password.message</message-key>
      <label-key>user.dong</label-key>
      <error-message-key>login.password.error</error-message-key>
      <type>password</type>
    </element>
  </elements>
</form>
