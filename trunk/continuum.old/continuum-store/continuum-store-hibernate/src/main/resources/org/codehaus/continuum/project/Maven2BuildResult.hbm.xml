<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

  <class
    name="org.codehaus.continuum.builder.maven2.Maven2BuildResult"
    table="Maven2BuildResult">
    <id name="buildId" type="java.lang.String">
      <generator class="assigned"/>
    </id>
    <property
      name="success"
      type="boolean"/>
    <!--
    <property
      name="executionResponse"
      type="org.codehaus.continuum.store.hibernate.ExecutionResponseType"/>
    -->
    <property
      name="executionResponse"
      type="java.lang.String"/>
  </class>

</hibernate-mapping>
