<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
  <class 
    name="org.codehaus.continuum.project.GenericBuildResult"
    table="BuildResult">
    <id name="buildId" type="java.lang.String">
      <generator class="uuid.hex"/>
    </id>
    <property name="state" type="int" not-null="true"/>
    <property name="startTime" type="long"/>
    <property name="endTime" type="long"/>
    <many-to-one
      name="project"
      class="org.codehaus.continuum.project.GenericContinuumProject"
      column="projectId"
      not-null="true"/>
  </class>
</hibernate-mapping>
