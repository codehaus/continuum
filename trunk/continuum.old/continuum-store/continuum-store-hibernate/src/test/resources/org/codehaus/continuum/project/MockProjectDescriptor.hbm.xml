<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
<!--
    extends="org.codehaus.continuum.project.AbstractProjectDescriptor"
-->
  <class
    name="org.codehaus.continuum.project.MockProjectDescriptor"
    discriminator-value="mock">
    <id name="projectId" type="java.lang.String">
      <generator class="assigned"/>
    </id>
    <property name="attribute" type="int" not-null="true"/>
    <one-to-one name="project" class="org.codehaus.continuum.project.GenericContinuumProject" constrained="true"/>
  </class>
</hibernate-mapping>
