      <name>ContinuumProject</name>
      <version>1.0.0</version>
      <fields>
        <field>
          <name>id</name>
          <required>true</required>
          <version>1.0.0</version>
          <type>String</type>
        </field>
        <field>
          <name>name</name>
          <version>1.0.0</version>
