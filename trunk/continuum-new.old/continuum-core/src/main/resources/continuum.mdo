    <!--
    |
    | ContinuumBuild
    |
    -->
    <class stash.storable="true">
      <name>ContinuumBuild</name>
      <version>1.0.0</version>
      <description>
        This class is a single continuum build.

        It has a reference to a {@link BuilderBuildResult} which contains
        any builder specific objects/variables.</description>
      <fields>
        <field>
          <name>project</name>
          <version>1.0.0</version>
          <association>
            <type>ContinuumProject</type>
          </association>
        </field>
        <field>
          <name>id</name>
          <version>1.0.0</version>
          <type>String</type>
        </field>
        <field>
          <name>state</name>
          <version>1.0.0</version>
          <type>int</type>
          <!--
          <association>
            <type>BuildResultState</type>
          </association>
          -->
        </field>
        <field>
          <name>startTime</name>
          <version>1.0.0</version>
          <type>long</type>
        </field>
        <field>
          <name>endTime</name>
          <version>1.0.0</version>
          <type>long</type>
        </field>
        <field stash.maxSize="8000">
          <name>error</name>
          <version>1.0.0</version>
          <type>String</type>
          <!--
          <association>
            <type>java.lang.Throwable</type>
          </association>
          -->
        </field>

        <!--
         |
         | Common fields for all builders
         |
         | -->

